{
   "name": "i18n Demo",
   "version": "1.0.0",
   "identifier": "app.here.i18n-demo",
   "description": "A i18n Demo, Just poc",
   "icon": "icon.png",
   "createdBy": "Lifesign",
   "website": "https://github.com/FriendsOfHere/i18n-demo",
   "appcast": "https://raw.githubusercontent.com/FriendsOfHere/i18n-demo/master/appcast.xml",
   "script": "index.js",
   "defaultCategory": "fav",
   "displayOn": [],
   "preferences": [
      {
          "title": "{{i18nTitle}}",
          "type": "popup",
          "key": "prefLang",
          "default": {{i18nDefault}},
            "options": [
                "English",
                "中文"
            ]
      },
      {
          "title": "{{i18nFood}}",
          "type": "popup",
          "key": "prefFood",
          "default": 0,
            "options": [
                "{{i18nBanana}}",
                "{{i18nApple}}"
            ]
      }
   ]
}
